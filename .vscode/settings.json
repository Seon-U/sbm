{
  "workbench.startupEditor": "newUntitledFile",
  "python.showStartPage": false,
  "editor.fontFamily": "Fira Code, \"Cascadia Code\"",
  "kite.showWelcomeNotificationOnStartup": false,
  "python.testing.pytestEnabled": true,
  "diffEditor.wordWrap": "on",
  "editor.wordWrap": "on",
  "editor.fontLigatures": true,
  "workbench.iconTheme": "material-icon-theme",
  "vsicons.dontShowNewVersionMessage": true,
  "workbench.colorCustomizations": {
    "[Material Theme]": {}
  },
  "window.title": "${dirty} ${activeEditorMedium}${separator}${rootPath}",
  "gitlens.advanced.messages": {
    "suppressCreatePullRequestPrompt": true,
    "suppressGitDisabledWarning": true,
    "suppressRebaseSwitchToTextWarning": true
  },
  "git.autofetch": true,
  "[git-rebase]": {
    "workbench.editor.restoreViewState": false
  },
  "gitlens.views.repositories.branches.layout": "list",
  "python.testing.cwd": "",
  "testing.automaticallyOpenPeekViewDuringAutoRun": true,
  "terminal.integrated.fontFamily": "monospace",
  "editor.inlineHints.fontSize": 4,
  "files.exclude": {
    ".vscode": true,
    "**/images/": true,
    "node_modules": true,
    "package-lock.json": true,
    "yarn.lock": true
  },
  "files.autoSave": "onFocusChange",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.snippetSuggestions": "top",
  "better-comments.highlightPlainText": true,
  "emmet.triggerExpansionOnTab": true,
  "emmet.syntaxProfiles": {},
  "emmet.preferences": {},
  "[scss]": {
    "editor.suggest.insertMode": "replace",
    "gitlens.codeLens.scopes": ["document"]
  },
  "liveServer.settings.donotShowInfoMsg": true,
  "liveSassCompile.settings.formats": [
    {
      "format": "compressed",
      "extensionName": ".min.css",
      "savePath": "/dist"
    }
  ],
  "liveSassCompile.settings.generateMap": false,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[markdown]": {
    "editor.defaultFormatter": null
  },
  "[css]": {
    "editor.defaultFormatter": null
  },
  "cSpell.userWords": ["BUYCNT", "Hometelier", "playjs", "ssac"],
  "prettier.singleQuote": true,
  "prettier.formatOnSave": true,
  "prettier.jsxSingleQuote": true,
  "prettier.arrowParens": "avoid",
  "turboConsoleLog.quote": "'",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "launch": {
    "configurations": [],
    "compounds": []
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always",
    "source.organizeImports.biome": "explicit",
    "quickfix.biome": "explicit",
    "source.fixAll.biome": "explicit",
    "source.sortImports": "explicit",
    "source.removeUnusedImports": "explicit"
  },
  "eslint.useFlatConfig": true,
  "editor.showUnused": true,
  "editor.codeLens": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "*html": "html"
  },
  "turboConsoleLog.insertEnclosingClass": false,
  "turboConsoleLog.insertEnclosingFunction": false,
  "turboConsoleLog.delimiterInsideMessage": "",
  "git.enableSmartCommit": true,
  "turboConsoleLog.includeFileNameAndLineNum": false,
  "security.workspace.trust.untrustedFiles": "open",

  "[typescript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[prisma]": { "editor.defaultFormatter": "Prisma.prisma" },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "scm.inputFontSize": 12,
  "editor.minimap.enabled": false,
  "markdown.preview.fontSize": 12,
  "workbench.editor.enablePreview": false,
  "editor.formatOnType": true,
  "terminal.integrated.fontWeight": "bold",
  "editor.tabCompletion": "on",
  "editor.formatOnSave": true,
  "terminal.integrated.copyOnSelection": true,
  "tabnine.experimentalAutoImports": true,
  "editor.stickyScroll.enabled": true,
  "editor.formatOnPaste": true,
  "search.exclude": {
    "**/__tests__": true,
    "**/build": true,
    "**/dist": true,
    "**/libs": true,
    "**/node_modules/**": true
  },
  "explorer.confirmDelete": false,
  "editor.quickSuggestions": {
    "strings": true
  },
  "tailwindCSS.experimental.configFile": null,
  "explorer.confirmDragAndDrop": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "terminal.integrated.env.osx": {},
  "notebook.output.scrolling": true,
  "interactiveWindow.alwaysScrollOnNewCell": false,
  "editor.tabSize": 2,
  "rest-client.environmentVariables": {
    "$shared": {}
  },
  "tailwindCSS.rootFontSize": 12,
  "console-ninja.fontSize": 12,
  "git.openRepositoryInParentFolders": "never",
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "workbench.editor.empty.hint": "hidden",
  "gitlens.views.commitDetails.files.layout": "list",
  "gitlens.graph.minimap.additionalTypes": [
    "localBranches",
    "stashes",
    "remoteBranches"
  ],
  "window.zoomLevel": 1,
  "workbench.tree.indent": 14,
  "css.enabledLanguages": ["html"],
  "css.styleSheets": [
    // "https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css",
    // "node_modules/bootstrap/dist/css/bootstrap.css",
    "src/**/*.scss",
    "src/view/style.ts",
    "dist/style.css",
    "htmlcss/*.css"
  ],
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[postcss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "prisma.showPrismaDataPlatformNotification": false,
  "projectManager.sortList": "Path",
  "explorer.confirmPasteNative": false,
  "tailwindCSS.classAttributes": [
    "class",
    "className",
    "ngClass",
    "class:list",
    "variants"
  ],
  "print.folder.fileNames": true,
  "print.folder.include": [],
  "print.folder.exclude": [
    "{bin,obj,out}",
    "node_modules",
    "data",
    "**/*.{bin,exe,dll,hex,pdb,pdf,pfx,png,jpg,gif,bmp,suo,design}"
  ],
  "print.folder.maxLines": 1200,
  "print.folder.maxFiles": 30,
  "print.folder.includeFileList": false,
  "code-runner.saveFileBeforeRun": true,
  "editor.cursorBlinking": "expand",
  "editor.cursorSmoothCaretAnimation": "on",
  "files.autoSaveDelay": 100,
  "github.copilot.enable": {
    "*": false,
    "plaintext": false,
    "markdown": false,
    "scminput": false
  },
  "quokka.fontSize": 12,
  "turboConsoleLog.logType": "debug",
  "biome.suggestInstallingGlobally": false
}
